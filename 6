#include <stdio.h>
#include <math.h>

int main(){
    
    int num,i,flag;
    printf("enter the number to be checked\n");
    scanf("%d",&num);
    i=2;
    flag=0;
    
    while(i<=num-1)
    {
        if(num%i==0)
        {
            flag=1;
            break;
        }
        i++;
    }
    if(flag==0)
        printf("The entered number is a prime number\n");
    else
        printf("The entered number is not a prime number\n");
    
    return 0;
    
}
