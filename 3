#include <stdio.h>


int main(){
    
    int sum,i;
    sum=0;
    for(i=1;i<=20;i++)
        
    {sum=i+sum;}
    printf("The sum of the first 20 nummbers is %d\n",sum);
    
    return 0;
    
}

