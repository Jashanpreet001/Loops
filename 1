#include<stdio.h>
#include<math.h>
int main(){
    
    int i,n,sum;
    sum=0;
    printf("enter the nth term\n");
    scanf("%d",&n);
    
    for(i=1;i<=n;i++)
    {  sum=sum+pow(i,2);}
    
    printf("The sum of the first %d terms is %d\n",n,sum);
    
    return 0;
}
